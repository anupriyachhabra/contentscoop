<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<g:TabPanel ui:field="mainTab" styleName="result">
            <g:Tab text="Text" ui:field="Tab">
               <g:FlexTable ui:field="textResults"></g:FlexTable> 
            </g:Tab>
             <g:Tab text="Images">
               <g:FlexTable ui:field="imageResults"></g:FlexTable> 
            </g:Tab>
        </g:TabPanel>
	</g:HTMLPanel>
</ui:UiBinder> 